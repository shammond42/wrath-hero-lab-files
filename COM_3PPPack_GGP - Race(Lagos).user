    <tag group="IsRace" tag="r3PLagos"/>
    <tag group="FavClass" tag="cRanger"/>
    <eval phase="Final" priority="9050"><![CDATA[
      ~ Calculate the FCB Bonus &
      field[abValue].value += round(field[fcCount].value/field[abValue5].value, 0, -1)
      doneif (field[abValue].value = 0)

      var active as number

      ~ look for copies of the favored enemy adjustment
      foreach pick in hero from BaseInPlay where "thingid.pFavorEnem"
        ~ Save all active states
        active += eachpick.field[pIsOn].value
      nexteach

      ~ If we find one, and it's not active, then get out
      doneif (active = 0)

      hero.child[ArmorClass].field[tACDodge].value += field[abValue].value]]></eval>
    </thing>
  <thing id="fc3PShaLag" name="Lagos Shaman" description="Add +1/2 to the shaman’s effective class level for the purpose of determining her spirit animal‘s natural armor adjustment, Intelligence, and special abilities." compset="FavClass" uniqueness="unique">
    <fieldval field="fcName" value="+½ Spirit Animal"/>
    <fieldval field="abValue5" value="2"/>
    <usesource source="srcGGPLago"/>
    <tag group="FavClass" tag="cShaman"/>
    <tag group="IsRace" tag="r3PLagos"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="First" priority="1200"><![CDATA[
      ~ Calculate the FCB Bonus &
      field[abValue].value += round(field[fcCount].value/field[abValue5].value, 0, -1)
      doneif (field[abValue].value = 0)

      doneif (hero.childlives[cArcFamil] = 0)

      hero.childfound[cArcFamil].field[CompLevel].value += field[abValue].value]]></eval>
    </thing>
  <thing id="fc3PWitLag" name="Lagos Witch" description="Add +5 feet to the distance at which her familiar grants the Alertness feat (maximum +20 feet)." compset="FavClass" uniqueness="unique">
    <fieldval field="fcName" value="+5ft Familiar"/>
    <fieldval field="abValue5" value="1"/>
    <usesource source="srcGGPLago"/>
    <tag group="FavClass" tag="cWitch"/>
    <tag group="IsRace" tag="r3PLagos"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PreLevel" priority="10000"><![CDATA[
      ~ Calculate the FCB Bonus &
      field[abValue].value += round(field[fcCount].value/field[abValue5].value, 0, -1)
      doneif (field[abValue].value = 0)

      field[abValue2].value += minimum(field[abValue].value * 5,20)

      doneif (field[abValue2].value = 0)

      perform hero.childfound[cArcFNear].setfocus
      doneif (state.isfocus = 0)

      focus.field[DescAppend].text = "{b}Witch FCB Modification:{/b} Familiar must be within " & field[abValue2].value & " feet to gain the benefits of the Alertness feat."
      focus.field[actName].text = "Within " & field[abValue2].value & " feet"]]></eval>
    </thing>
  <thing id="fc3PVigLag" name="Lagos Vigilante" description="Add +1/3 to Acrobatics, Escape Artist, and Stealth in plains areas." compset="FavClass" uniqueness="unique">
    <fieldval field="fcName" value="+1/3 skills in plains"/>
    <fieldval field="abValue5" value="3"/>
    <usesource source="srcGGPLago"/>
    <tag group="FavClass" tag="cVigilante"/>
    <tag group="IsRace" tag="r3PLagos"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="Final" priority="50000"><![CDATA[
      ~ Calculate the FCB Bonus &
      field[abValue].value += round(field[fcCount].value/field[abValue5].value, 0, -1)
      doneif (field[abValue].value = 0)

      perform assign[Helper.ShowSpec]

      field[livename].text = signed(field[abValue].value) & " to Acrobatics, Escape Artist, and Stealth in plains"

      #situational[hero.childfound[skAcrobat], signed(field[abValue].value) & " bonus in plains.", field[thingname].text]
      #situational[hero.childfound[skEscape], signed(field[abValue].value) & " bonus in plains.", field[thingname].text]
      #situational[hero.childfound[skStealth], signed(field[abValue].value) & " bonus in plains.", field[thingname].text]]]></eval>
    </thing>
  <thing id="ar3PDriNom" name="Drift Nomad" description="The lagos live migratory lives. Since they never settle down for long, there would be very few opportunities for monastic traditions to be observed. However, a form of spirituality was born from lagos who developed the ‘zen’ of the open road. Passed on from generation to generation as the greatest means of self-reliance, travel, and acceptance of their race’s ways, a single drift nomad lago can cover an entire flank of a drove by themselves if necessary.\n\nOlder abandoned drift nomads are relatively common, as they themselves expect to leave the drove at some point. Some droves low in population or resources may decide to ignore abandonment traditions in order to retain particularly skilled monks, but usually a lago is left to learn the zen of the open road on their own." compset="ClassVary" uniqueness="unique">
    <usesource source="srcGGPLago"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="ClVaryName" tag="AppParen"/>
    <tag group="AlgnReq" tag="AnyNeutral"/>
    <tag group="fInclude" tag="fCatchOff"/>
    <tag group="fInclude" tag="fComRef"/>
    <tag group="fInclude" tag="fDefArrow"/>
    <tag group="fInclude" tag="fDodge"/>
    <tag group="fInclude" tag="fImpGrapp"/>
    <tag group="fInclude" tag="fThrowAny"/>
    <tag group="fInclude" tag="f3PWiHaSty"/>
    <tag group="AbReplace" tag="cMnkStun1"/>
    <tag group="AbReplace" tag="cMnkStill"/>
    <tag group="AbReplace" tag="cMnkTime"/>
    <bootstrap thing="c3PDriStri">
      <autotag group="ClSpecWhen" tag="1"/>
      </bootstrap>
    <bootstrap thing="c3PNomEndu">
      <autotag group="ClSpecWhen" tag="3"/>
      </bootstrap>
    <bootstrap thing="c3PDrifBli">
      <autotag group="ClSpecWhen" tag="17"/>
      </bootstrap>
    <link linkage="varies" thing="cHelpMnk"/>
    <eval phase="First" priority="1000">~ Remove normal list of feats
     perform linkage[varies].delete[BonusFeat.?]
     perform linkage[varies].delete[BonusFor.?]</eval>
    <exprreq message="Lagos race required."><![CDATA[#hasrace[r3PLagos] <> 0]]></exprreq>
    </thing>
  <thing id="f3PWiHaSty" name="Wild Hare Style" description="Passed on by lagos drift nomads, the wild hare style embodies the speed and momentum that the lagos can build while still avoiding harm.\n\n{b}Prerequisites:{/b} Dex 13, movement speed of at least 30.\n\n{b}Benefit:{/b} You do not suffer any penalty to AC from charging. You also keep your Dexterity bonus to AC when making a full round run action as the Run feat." compset="Feat" summary="No penalty to AC when charging and keep Dex to AC when running." uniqueness="useronce">
    <fieldval field="reqDex" value="13"/>
    <usesource source="srcGGPLago"/>
    <tag group="fCategory" tag="Combat"/>
    <tag group="fCategory" tag="Style"/>
    <tag group="fMoMSNext" tag="f3PWiHaJuk"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)

      perform hero.assign[Hero.RunningDex]

      #value2[pstChargin] = 0]]></eval>
    <exprreq message="Movement speed of at least 30 required."><![CDATA[hero.child[Speed].field[tSpeedMod].value >= 30]]></exprreq>
    </thing>
  <thing id="f3PWiHaJuk" name="Wild Hare Juke" description="Wild hares juke left or right while pursued, and easily can throw off pursuers. Imitating these movements while on the attack is equally effective.\n\n{b}Prerequisites:{/b} Dex 15, Wild Hare Style.\n\n{b}Benefit:{/b} You may make charges that change direction in surprising ways with no apparent loss in momentum. You do not need to select the straightest path to the target of your charge, but must travel in straight lines and you must reach the target by the end of twice your movement (on a full charge) or your movement (on a partial charge). You may change direction a number of times during your charge equal to your Dexterity modifier (minimum 1). Enemies cannot set to receive your charge when you use this feat." compset="Feat" summary="Change directions during a charge attack." uniqueness="useronce">
    <fieldval field="reqDex" value="15"/>
    <fieldval field="abText" value="change direction"/>
    <usesource source="srcGGPLago"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="fMoMSNext" tag="f3PWiHaImp"/>
    <tag group="fCategory" tag="Style"/>
    <tag group="ReqFeat" tag="f3PWiHaSty"/>
    <tag group="fCategory" tag="Combat"/>
    <tag group="LvNamePar" tag="AppText"/>
    <tag group="LvNamePar" tag="AppValue"/>
    <eval phase="PostAttr" priority="10000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)

      field[abValue].value += #attrbonus1[aDEX]]]></eval>
    </thing>
  <thing id="f3PWiHaImp" name="Wild Hare Impact" description="The great speed of mobile fighters are usually wasted just getting to their targets, but with proper technique, that momentum can be delivered into an enemy’s body.\n\n{b}Prerequisites:{/b} Dex 17, Power Attack, Wild Hare Juke.\n\n{b}Benefit:{/b} You throw every bit of momentum into your charge. When you successfully hit with a charge attack while using Power Attack, you may make a bull rush maneuver against the target as a free action, adding a +1 bonus to the CMB check for every 10 feet you traveled during your charge. This bull rush does not provoke attacks of opportunity." compset="Feat" summary="Charge with Power Attack make free bull rush adding +1 per 10ft moved." uniqueness="useronce">
    <fieldval field="reqDex" value="17"/>
    <usesource source="srcGGPLago"/>
    <tag group="fCategory" tag="Style"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="fCategory" tag="Combat"/>
    <tag group="ReqFeat" tag="fPowerAtt"/>
    <tag group="ReqFeat" tag="f3PWiHaJuk"/>
    </thing>
  <thing id="c3PDriStri" name="Drift Strike" description="At 1st level, a drift nomad who moves more than 10 feet in the round and makes an unarmed strike or natural attack adds their monk level to the damage. This ability replaces the monk’s stunning\nfist." compset="ClSpecial" summary="Moving more than 10ft gives +level on dmg for unarmed strike and natural weapons.">
    <fieldval field="actName" value="Moved more than 10ft"/>
    <tag group="User" tag="Activation"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="LvNamePar" tag="AppValue"/>
    <tag group="LvNamePar" tag="SignAppVal"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      ~ If we're not shown, just get out now &
      doneif (tagis[Helper.ShowSpec] <> 1)
      ~ if we've been disabled, get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      field[abValue].value += field[xTotalLev].value]]></eval>
    <eval phase="PostLevel" priority="15000" index="2"><![CDATA[
      ~ If we're not shown, just get out now &
      doneif (tagis[Helper.ShowSpec] <> 1)
      ~ if we've been disabled, get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      ~ The following should only happen once on the first copy
      doneif (tagis[Helper.FirstCopy] = 0)
      doneif (field[abilActive].value = 0)

      foreach pick in hero from BaseWep where "wFtrGroup.Natural"
        eachpick.field[wDamBonus].value += field[abValue].value
      nexteach]]></eval>
    </thing>
  <thing id="c3PNomEndu" name="Nomad’s Endurance" description="At 3rd level, the drift nomad gains Endurance as a bonus feat, and the feat bonus doubles when he makes Constitution checks because of a forced march. In addition, a drift nomad gains a +2 bonus on saving throws against spells and\neffects that cause exhaustion and fatigue. This ability replaces still mind." compset="ClSpecial">
    <fieldval field="abValue" value="4"/>
    <fieldval field="abValue" value="2"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <bootstrap thing="fEndure">
      <containerreq phase="First" priority="500"><![CDATA[      (count:Classes.Monk >= 3 | count:Classes.MonkUnc >= 3) & !AbReplace.c3PNomEndu]]></containerreq>
      </bootstrap>
    <eval phase="Final" priority="50000"><![CDATA[
      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] <> 1)
      ~ if we've been disabled, get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      ~ The following should only happen once on the first copy
      doneif (tagis[Helper.FirstCopy] = 0)

      #situational[hero.child[aCON],signed(field[abValue].value) & " bonus when doing a forced march (this stacks with endurance feat).",field[thingname].text]
      #situational[hero.child[aCON],signed(field[abValue2].value) & " vs. spells and effects that cause exhaustion and fatigue.",field[thingname].text]]]></eval>
    </thing>
  <thing id="c3PDrifBli" name="Drift Blitz" description="At 17th level, a drift nomad can make a full attack at the end of a charge. While making a full attack in this method, the damage bonus granted by drift strike is reduced to half their monk level. This ability replaces timeless body." compset="ClSpecial" summary="Full attack at end of a charge but drift strike dmg is 1/2.">
    <fieldval field="actName" value="1/2 drift strike dmg"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="User" tag="Activation"/>
    <tag group="AbilType" tag="Extra"/>
    <eval phase="PostLevel" priority="11000"><![CDATA[
      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] <> 1)
      ~ if we've been disabled, get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      ~ The following should only happen once on the first copy
      doneif (tagis[Helper.FirstCopy] = 0)
      doneif (field[abilActive].value = 0)

      ~ Set drift damage to half
      #value[c3PDriStri] = round(hero.childfound[c3PDriStri].field[abValue].value/2,0,-1)]]></eval>
    </thing>
  <thing id="w3PLegBrac" name="Leg Braces" compset="Weapon">
    <usesource source="srcGGPLago"/>
