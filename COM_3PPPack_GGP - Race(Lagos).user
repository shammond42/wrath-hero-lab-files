    <tag group="Helper" tag="SpecUp"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.SpcDisable] <> 0)

      hero.childfound[ra3PSprtJu].field[DescAppend].text = "{b}Sure-Footed Modification:{/b} No longer gain the +10 foot racial bonus to speed when using the charge, run or withdraw action."
      #value[ra3PSprtJu] = 0]]></eval>
    <eval phase="Final" priority="50000" index="2"><![CDATA[
      ~ If we're disabled, do nothing
