    <bootstrap thing="fAlertness">
      <containerreq phase="First" priority="700">!AbReplace.ra3PAlrtDa</containerreq>
      </bootstrap>
    <eval phase="PreLevel" priority="10000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.SpcDisable] <> 0)

      #makeclassskill[skPercep]
      #makeclassskill[skSenseMot]]]></eval>
    </thing>
