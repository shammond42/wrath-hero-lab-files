    <tag group="Helper" tag="EquipMag" name="EquipMag" abbrev="EquipMag"/>
    <tag group="Helper" tag="ArtiMinor"/>
    <tag group="Helper" tag="LegendItem"/>
    <eval phase="PreAttr" priority="10000"><![CDATA[
      ~ Items which would add enhancement bonuses to attributes do not do so
      ~ when we are using the Auto Bonus Progression rules from Pathfinder Unchained.
      doneif (hero.tagis[source.PUAutoB?] <> 0)

      doneif (field[gIsEquip].value = 0)

      #enhancementbonus[hero.child[aINT], 2]
      #enhancementbonus[hero.child[aWIS], 2]
      #enhancementbonus[hero.child[aCHA], 4]

      ~ add to our languages known
      herofield[tLangsSpk].value += 1]]></eval>
    <eval phase="PostLevel" priority="4000" index="2"><![CDATA[
      ~ Items which would add enhancement bonuses to attributes do not do so
      ~ when we are using the Auto Bonus Progression rules from Pathfinder Unchained.
      doneif (hero.tagis[source.PUAutoB?] <> 0)

      doneif (field[gIsEquip].value = 0)

      ~ now, we'll add item ranks to that skill equal to our HD
      if (field[usrChosen1].ischosen <> 0) then
        field[usrChosen1].chosen.field[skItem].value += herofield[tHitDice].value
        endif]]></eval>
    </thing>
