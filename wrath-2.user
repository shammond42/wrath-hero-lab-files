  <thing id="trWeapProf" name="Weapon Proficiency" description="Choose one type of weapon, such as the spiked chain or whip. You understand how to use that type of exotic weapon in combat, and can utilize any special tricks or qualities that exotic weapon might allow.\n\n{b}Benefit{/b}: You make attack rolls with the weapon normally.\n\n{b}Normal{/b}: A character who uses a weapon with which he is not proficient takes a –4 penalty on attack rolls.\n\n{b}Special{/b}: You can gain the Weapon Proficiency Trait multiple times. Each time you take the feat, it applies to a new type of exotic weapon." compset="Trait" summary="You are proficient with one weapon you normally wouldn&apos;t be." uniqueness="useronce">
    <usesource source="WotR2Traits"/>
    <tag group="fShowWhat" tag="Weapons"/>
    <tag group="ChooseSrc1" tag="Thing"/>
    <tag group="Custom" tag="WeaponProf"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="trCategory" tag="Combat"/>
    <eval phase="PreLevel" priority="5000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)

      ~ If we haven't chosen anything, get out now
      doneif (field[usrChosen1].ischosen + tagis[Target.?] = 0)

      if (tagcount[Hero.MartVersa] + hero.tagcount[Hero.MartMaster] <> 0) then
        perform field[usrChosen1].chosen.pulltags[wFtrGroup.?]

        ~ this call takes care of mythic martial versatility and mythic martial mastery
        call MytMartial

        foreach thing in BaseWep where "wProfReq.Exotic & (" & tagids[wFtrGroup.?,"|"] & ")"
          perform eachthing.pulltags[WepProf.?]
          nexteach

        perform hero.pushtags[WepProf.?]

        foreach pick in hero from BaseWep
          if (eachpick.tagmatch[wFtrGroup,wFtrGroup,initial] <> 0) then
            perform eachpick.assign[Helper.ExoticProf]
            endif
          nexteach
      else
        ~ Assign the appropriate tag to all weapons that meet the criteria
        var id as string
        var name as string
        call fTargetId

        foreach pick in hero where "IsWeapon." & id
          perform eachpick.assign[Broadcast.WepProf]
          perform eachpick.assign[Helper.ExoticProf]
          perform eachpick.pushtags[Broadcast.?]
          nexteach

        ~ Set our 'short name'
        field[shortname].text = "EWP: " & name
        field[sbName].text = "Exotic Weapon Proficiency (" & lowercase(name) & ")"

        ~ Forward the weapon specialization tag for the weapon to the hero
        perform hero.assignstr["WepProf." & id]
        endif

      ~ Pull the IsWeapon tag if we choose quarterstaff so that we can detect
      ~ that in the pre-req for this feat.
      if (field[usrChosen1].chosen.tagis[IsWeapon.wQtrstaff] <> 0) then
        perform field[usrChosen1].chosen.pulltags[IsWeapon.?]
        endif]]></eval>
    </thing>
